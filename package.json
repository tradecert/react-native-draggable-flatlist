    "prettier": "^2.2.1",
    "pretty-quick": "^2.0.1",
    "react": "^17.0.2",
    "react-native": "^0.64.0",
    "react-native-builder-bob": "^0.18.2",
    "react-native-gesture-handler": "^2.1.0",
    "react-native-reanimated": "^2.8.0",
    "react-test-renderer": "^17.0.2",
    "typescript": "^4.2.4"
  },
  "react-native-builder-bob": {
    "source": "src",
